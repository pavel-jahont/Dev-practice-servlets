<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>

    <display-name>Archetype Created Web Application</display-name>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>InitServlet</servlet-name>
        <servlet-class>com.gmail.jahont.pavel.app.controller.InitServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InitServlet</servlet-name>
        <url-pattern>/init</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowAllUsersServlet</servlet-name>
        <servlet-class>com.gmail.jahont.pavel.app.controller.ShowAllUsersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowAllUsersServlet</servlet-name>
        <url-pattern>/users</url-pattern>
    </servlet-mapping>

</web-app>